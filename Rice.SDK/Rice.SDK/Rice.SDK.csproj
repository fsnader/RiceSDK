<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <PackageId>Rice.SDK</PackageId>
    <Version>1.0.0</Version>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <Authors>Felipe Nader</Authors>
    <PackageProjectUrl>https://github.com/fsnader/RiceSDK</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AutoMapper.Extensions.Microsoft.DependencyInjection" Version="6.0.0" />
    <PackageReference Include="NLog.Web.AspNetCore" Version="4.8.1" />
    <PackageReference Include="System.Linq.Dynamic.Core" Version="1.0.9" />
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="2.1.4" />
    <PackageReference Include="Microsoft.IdentityModel.Tokens" Version="5.1.4" />
    <PackageReference Include="System.IdentityModel.Tokens.Jwt" Version="5.1.4" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="Logger\**" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Remove="Logger\**" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Logger\**" />
  </ItemGroup>
</Project>
